<demos>
    <name>Communications System Toolbox Support Package for Xilinx Zynq-Based Radio</name>
    <type>matlab</type>
    <icon>$toolbox/matlab/icons/matlabicon.gif</icon>
    <description isCdata="no">
        <p>Communications System Toolbox Support Package for Xilinx Zynq-Based Radio provides a library of blocks and System objects 
            to utilize SDR hardware for rapid prototyping.
        </p>
    </description>
  
    <demosection>
        <label>Simulink Examples for AD9361/AD9364</label>
        <demosection>
            <label>Diagnostics</label>
            <demoitem>
                <label>Frequency Offset Calibration Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <type>model</type>
                <source>zynqRadioFrequencyCalibrationTxAD9361AD9364SL</source>
                <file>html/zynqRadioFrequencyCalibrationTxAD9361AD9364SL.html</file> 
            </demoitem>  
        </demosection>        
       
        <demosection>
            <label>Hardware/Software Co-design</label>
            <demoitem>
                <label>HW/SW Co-design QPSK Transmit and Receive Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <type>model</type>
                <source>zynqRadioHWSWQPSKAD9361AD9364SL</source>
                <file>html/zynqRadioHWSWQPSKAD9361AD9364SL.html</file> 
            </demoitem>  
            <demoitem>
                <label>HW/SW Co-design Implementation of LTE OFDM Detector Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <dependency>LTE System Toolbox</dependency>
                <type>model</type>
                <source>zynqRadioHWSWLTEDetectorAD9361AD9364SL</source>
                <file>html/zynqRadioHWSWLTEDetectorAD9361AD9364SL.html</file> 
            </demoitem>
        </demosection>
        
        <demosection>
            <label>Digital Modulation</label>
            <demoitem>
                <label>QPSK Transmitter Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <type>model</type>
                <source>zynqRadioQPSKTxAD9361AD9364SL</source>
                <file>html/zynqRadioQPSKTxAD9361AD9364SL.html</file> 
            </demoitem>
            <demoitem>
                <label>QPSK Receiver Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <type>model</type>
                <source>zynqRadioQPSKRxAD9361AD9364SL</source>
                <file>html/zynqRadioQPSKRxAD9361AD9364SL.html</file> 
            </demoitem>
        </demosection>
        
        <demosection>
            <label>HDL</label>
            <demoitem>
                <label>Targeting HDL Optimized QPSK Transmitter Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <type>model</type>
                <source>zynqRadioQPSKTxFPGAAD9361AD9364SL</source>
                <file>html/zynqRadioQPSKTxFPGAAD9361AD9364SL.html</file> 
            </demoitem>
            <demoitem>
                <label>Targeting HDL Optimized QPSK Receiver Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <type>model</type>
                <source>zynqRadioQPSKRxFPGAAD9361AD9364SL</source>
                <file>html/zynqRadioQPSKRxFPGAAD9361AD9364SL.html</file> 
            </demoitem>   	
            <demoitem>
                <label>Targeting the HDL Optimized 802.11 WLAN Beacon Frame Receiver Using Analog Devices AD9361/AD9364</label>
                <dependency>Simulink</dependency>
                <type>model</type>
                <source>zynqRadioWLAN80211BeaconRxFPGAAD9361AD9364SL</source>
                <file>html/zynqRadioWLAN80211BeaconRxFPGAAD9361AD9364SL.html</file> 
            </demoitem>
        </demosection>
        
